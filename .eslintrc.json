{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/prop-types": 0,
        "no-invalid-this": 0,
        "no-unused-vars": 0,
        "react/no-unescaped-entities": 0,
        "require-jsdoc": 0,
        "max-len": 0
    }
}
